<h1>【MHXX】お気に入り並び替え</h1>
<div class="mb-2">
  <%= link_to "クエスト一覧へ戻る", mhxx_quests_path, class: "btn btn-info" %>
</div>
<ul class="list-group" id="sort-table">
  <% @bookmarks.each do |bookmark_quest| %>
    <li href="#" class="list-group-item list-group-item-action" data-id="<%= bookmark_quest.id %>">
      <%= bookmark_quest.m_quest.name %>
    </li>
  <% end %>
</ul>
