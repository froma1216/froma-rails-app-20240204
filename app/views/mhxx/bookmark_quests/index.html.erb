<h1>【MHXX】お気に入り並び替え</h1>
<div class="mb-2">
  <%= link_to "クエスト一覧へ戻る", mhxx_quests_path, class: "btn btn-info" %>
</div>
<div class="list-group" id="sort-table">
  <% @bookmarks.each do |bookmark_quest| %>
    <button type="button" class="list-group-item list-group-item-action" data-id="<%= bookmark_quest.id %>">
      <i class="fa-solid fa-grip-vertical text-black-50 me-4"></i><%= bookmark_quest.m_quest.name %>
    </button>
  <% end %>
</div>
